<table class="styled-table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Listen</th>
    </tr>
  </thead>
  <tbody>

    <%  trackItems.forEach((track)=>{ %>
    <tr>
      <td>
        <p><%- track.name %></p>
      </td>
      <td> <audio controls src="<%- track.preview_url  %>">> <a href="<%- track.preview_url  %>">Download audio</a></audio>
        <%})
        %> </td>
    </tr>
  </tbody>
</table>
