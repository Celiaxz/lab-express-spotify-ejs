<!-- <h1>Tracks</h1> -->
<table class="styled-table">
  <thead>
    <tr class="table-titles">
      <th>#</th>
      <th>Title</th>
      <th>Listen</th>
    </tr>
  </thead>
  <tbody>

    <%  trackItems.forEach((track, index)=>{ %>
    <tr>
      <td>
        <p><%- index + 1 %></p>
      </td>
      <td>
        <p><%- track.name %></p>
      </td>
      <td> <audio controls src="<%- track.preview_url  %>">> <a href="<%- track.preview_url  %>">Download audio</a></audio>
        <%})
        %> </td>
    </tr>
  </tbody>
</table>